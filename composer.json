{
    "name": "spekulatius/gitpolicy",
    "description": "Assists your developers and contributors with sensible git usage",
    "homepage": "https://peterthaleikis.com",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.2",
        "symfony/console": "^5",
        "symfony/yaml": "^5.0",
        "symfony/event-dispatcher": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^3.0"
